{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "configs": ["src/configs"],
      "configs/*": ["src/configs/*"],
      "contexts": ["src/contexts"],
      "contexts/*": ["src/contexts/*"],
      "enums": ["src/enums"],
      "enums/*": ["src/enums/*"],
      "lotties": ["src/lotties"],
